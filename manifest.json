{
  "manifest_version": 2,
  "name": "PrimoEden",
  "description": "Background Worker",
  "version": "1.0",
  "icons": {
    "32": "32.png"
  },
  "background": { "page": "background.html"},
  "permissions": [ 
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://api.twitter.com; object-src 'self'",
  "browser_action": {
    "default_icon": "32.png",
    "default_popup": "popup.html"
  }
}
